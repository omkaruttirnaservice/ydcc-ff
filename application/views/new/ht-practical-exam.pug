doctype html
html(lang="en")
  head
    meta(charset="utf-8")
    meta(name="viewport" content="width=device-width, initial-scale=1")
    title #{details.ub_first_name}_#{details.ub_middle_name}_#{details.ub_last_name}
    link(
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH"
      crossorigin="anonymous"
    )

    style.
        body {
          font-size: 13px;
        }
    
        @media print {
            .printBtn {
                display:none;
            }
            table,.table,th,td,.table td, .table th{
            border-color:black !important
            }
        }
      td {
        padding: 5px;
      }
  body
    button.btn.btn-success.ms-4.mt-4.text-center.printBtn(onclick="window.print()") Print Proficiency Test Hallticket
    .p-2.position-relative(style="width: 200mm; height: 200mm")
