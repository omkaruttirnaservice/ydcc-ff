        .d-flex.gap-3.justify-content-center.photo-container(style="")
          .border(style="width: 6.9rem; height: 100%")
              img(src=s3BucketUrl+'/'+details.ca_photo, alt="" class="w-100 h-100")
          .border(style="width: 5.7rem; height: 100%")
